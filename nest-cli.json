{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"compilerOptions": {
		"deleteOutDir": true,
		"builder": {
			"type": "swc",
			"options": {
				"swcrcPath": "/.swcrc.json",
				"extensions": [".js", ".ts", ".jsx", ".tsx"]
			}
		},
		"typeCheck": true
	}
}
